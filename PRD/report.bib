@article{2041-8205-833-1-L1,
	Abstract = {A transient gravitational-wave signal, GW150914, was identified in the twin Advanced LIGO detectors on 2015 September 2015 at 09:50:45 UTC. To assess the implications of this discovery, the detectors remained in operation with unchanged configurations over a period of 39 days around the time of the signal. At the detection statistic threshold corresponding to that observed for GW150914, our search of the 16 days of simultaneous two-detector observational data is estimated to have a false-alarm rate (FAR) of ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn1.gif] {$\lt 4.9\times {10}^{-6}\,{\mathrm{yr}}^{-1}$} , yielding a p -value for GW150914 of ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn2.gif] {$\lt 2\times {10}^{-7}$} . Parameter estimation follow-up on this trigger identifies its source as a binary black hole (BBH) merger with component masses ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn3.gif] {$({m}_{1},{m}_{2})=({36}_{-4}^{+5},{29}_{-4}^{+4})\,{M}_{\odot }$} at redshift ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn4.gif] {$z={0.09}_{-0.04}^{+0.03}$} (median and 90% credible range). Here, we report on the constraints these observations place on the rate of BBH coalescences. Considering only GW150914, assuming that all BBHs in the universe have the same masses and spins as this event, imposing a search FAR threshold of 1 per 100 years, and assuming that the BBH merger rate is constant in the comoving frame, we infer a 90% credible range of merger rates between ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn5.gif] {$2\mbox{--}53\,{\mathrm{Gpc}}^{-3}\,{\mathrm{yr}}^{-1}$} (comoving frame). Incorporating all search triggers that pass a much lower threshold while accounting for the uncertainty in the astrophysical origin of each trigger, we estimate a higher rate, ranging from ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn6.gif] {$13\mbox{--}600\,{\mathrm{Gpc}}^{-3}\,{\mathrm{yr}}^{-1}$} depending on assumptions about the BBH mass distribution. All together, our various rate estimates fall in the conservative range ##IMG## [http://ej.iop.org/images/2041-8205/833/1/L1/apjlaa4677ieqn7.gif] {$2\mbox{--}600\,{\mathrm{Gpc}}^{-3}\,{\mathrm{yr}}^{-1}$} .},
	Author = {B. P. Abbott and others},
	Date-Added = {2018-06-20 09:46:52 +0000},
	Date-Modified = {2018-06-20 09:47:01 +0000},
	Journal = {The Astrophysical Journal Letters},
	Number = {1},
	Pages = {L1},
	Title = {The Rate of Binary Black Hole Mergers Inferred from Advanced LIGO Observations Surrounding GW150914},
	Url = {http://stacks.iop.org/2041-8205/833/i=1/a=L1},
	Volume = {833},
	Year = {2016},
	Bdsk-Url-1 = {http://stacks.iop.org/2041-8205/833/i=1/a=L1}}

